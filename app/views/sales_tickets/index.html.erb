<% @locations.each do |location| %>
  <table class="table table-striped table-condensed table-hover table-bordered">
    <caption><%= location.description %> Sales Tickets</caption>
    <thead>
      <tr>
        <th>multi_payment</th>
        <th>payment_type</th>
        <th>refunds_returns_allowed</th>
        <th>sales_person_id</th>
        <th>synchronized</th>
        <th>tax_amount</th>
        <th>tax_rate</th>
      </tr>
    </thead>
    <tbody>
    <% location.sales_tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.multi_payment -%></td>
        <td><%= ticket.payment_type -%></td>
        <td><%= ticket.refunds_returns_allowed -%></td>
        <td><%= ticket.sales_person_id -%></td>
        <td><%= ticket.synchronized -%></td>
        <td><%= ticket.tax_amount -%></td>
        <td><%= ticket.tax_rate -%></td>
      </tr>
    <% end %>
    </tbody>
  </table>
<% end %>

