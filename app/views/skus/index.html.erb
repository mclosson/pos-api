<table id="locations" class="display table">
  <thead>
    <tr>
      <th>Article</th>
      <th>Color</th>
      <th>Cost Price</th>
      <th>Cost Price IGIC</th>
      <th>Description</th>
      <th>Gender</th>
      <th>Inactive</th>
    </tr>
  </thead>
  <tbody>
  <% @skus.each do |sku| %>
    <tr>
      <td><%= sku.article.description -%></td>
      <td><%= sku.color -%></td>
      <td><%= sku.cost_price -%></td>
      <td><%= sku.cost_price_igic -%></td>
      <td><%= sku.description -%></td>
      <td><%= sku.gender.description -%></td>
      <td><%= sku.inactive -%></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br />
<hr>

<p>
  <%= link_to 'New SKU', new_sku_url %>
</p>

